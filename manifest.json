{
  "manifest_version": 2,
  "name": "BRAVO",
  "description": "An extension to visualize your internet usage",
  "version": ".1",

  "browser_action": {
    "default_icon": { 
      "icon.png" 
    },
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "activeTab",
    "browsingData",
    "cookies",
    "history",
    "sessions",
    "tabs"
  ],
}
