{
    "manifest_version": 2,

    "name": "Listen",
    "description": "This is a sample chrome extension",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["listen.js"]
    }
}
